edIn: false,\n      currentPage:'',\n    };\n  },\n  computed: {\n    loginText() {\n      return this.loggedIn ? 'Logout' : 'Login';\n    },\n    showLoginButton(){\n      return this.currentPage !== 'LoginClient-page';\n    }\n  },\n  created() {\n\n    const storedLoggedIn = localStorage.getItem('loggedIn');\n    if (storedLoggedIn) {\n      this.loggedIn = JSON.parse(storedLoggedIn);\n    }\n  },\n  methods: {\n    handleLoginLogout() {\n      if (this.loggedIn) {\n        this.logout();\n\n      } else {\n        this.$router.push('/login');\n\n      }\n    },\n    logout() {\n\n      localStorage.removeItem('client');\n      this.loggedIn = false;\n      localStorage.setItem('loggedIn', JSON.stringify(this.loggedIn));\n      this.$router.push('/Orders');\n    },\n    updateCurrentPage(route)\n    {\n      this.currentPage =route.name;\n    }\n\n  },\n};\n</script>\n\n<style>\n/*\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n\n}*/\n</style>\n"],"mappings":";AA0EA,eAAe;EACbA,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE,CAAC,CAAC;EAEdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,WAAW,EAAC;IACd,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAACH,QAAO,GAAI,QAAO,GAAI,OAAO;IAC3C,CAAC;IACDI,eAAeA,CAAA,EAAE;MACf,OAAO,IAAI,CAACH,WAAU,KAAM,kBAAkB;IAChD;EACF,CAAC;EACDI,OAAOA,CAAA,EAAG;IAER,MAAMC,cAAa,GAAIC,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;IACvD,IAAIF,cAAc,EAAE;MAClB,IAAI,CAACN,QAAO,GAAIS,IAAI,CAACC,KAAK,CAACJ,cAAc,CAAC;IAC5C;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,iBAAiBA,CAAA,EAAG;MAClB,IAAI,IAAI,CAACZ,QAAQ,EAAE;QACjB,IAAI,CAACa,MAAM,CAAC,CAAC;MAEf,OAAO;QACL,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;MAE7B;IACF,CAAC;IACDF,MAAMA,CAAA,EAAG;MAEPN,YAAY,CAACS,UAAU,CAAC,QAAQ,CAAC;MACjC,IAAI,CAAChB,QAAO,GAAI,KAAK;MACrBO,YAAY,CAACU,OAAO,CAAC,UAAU,EAAER,IAAI,CAACS,SAAS,CAAC,IAAI,CAAClB,QAAQ,CAAC,CAAC;MAC/D,IAAI,CAACc,OAAO,CAACC,IAAI,CAAC,