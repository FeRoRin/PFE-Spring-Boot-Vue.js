{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"Oredr-page\",\n  data() {\n    return {\n      client: {},\n      // LocalStorage\n      orders: []\n    };\n  },\n  created() {\n    // Get back LocalStorage\n    const clientData = JSON.parse(localStorage.getItem('client'));\n    if (clientData) {\n      this.client = clientData;\n    }\n  },\n  methods: {\n    redirectToProduct(orderId) {\n      localStorage.setItem(\"selectedOrderId\", orderId);\n      this.$router.push(\"/products_order\");\n    },\n    logout() {\n      // Clear LocalStorage\n      localStorage.removeItem('client');\n      localStorage.setItem('loggedIn', false);\n      this.$router.push('/about');\n    },\n    retrieveOrder() {\n      if (this.client) {\n        axios.get(`http://localhost:8080/Client/${this.client.id}/Commande`).then(response => {\n          this.orders = response.data;\n        }).catch(error => {\n          console.error(\"\", error);\n        });\n      } else {\n        console.error('');\n      }\n    },\n    deleteOrder(orderId) {\n      axios.delete(`http://localhost:8080/Commande/${orderId}`).then(response => {\n        console.log(response);\n        window.location.reload();\n      }).catch(error => {\n        console.error(\" delete failed !!! \", error);\n      });\n    }\n  },\n  mounted() {\n    this.retrieveOrder();\n  }\n};","map":{"version":3,"names":["axios","name","data","client","orders","created","clientData","JSON","parse","localStorage","getItem","methods","redirectToProduct","orderId","setItem","$router","push","logout","removeItem","retrieveOrder","get","id","then","response","catch","error","console","deleteOrder","delete","log","window","location","reload","mounted"],"sources":["C:\\Users\\Firdaouss\\Desktop\\vue\\front-end\\front-end\\src\\components\\Client\\ClientOrderList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Welcome <b class=\"name-cl\">{{ client.firstName }}</b> to Your Order List</h1>\r\n    <div>\r\n      <p><strong>Email:</strong> {{ client.email }}</p>\r\n    </div>\r\n    <!--button @click=\"logout\">LogOut</button-->\r\n    <div class=\"d-sm-flex align-items-center justify-content-between mb-4\">\r\n      <h1 class=\"h3 mb-0 text-gray-800\">Dashboard</h1>\r\n      \r\n      <button class=\"d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm\" @click=\"addNewOrder(order.id)\">\r\n      <i class=\"fas fa-download fa-sm text-white-50\"></i>\r\n              New Order </button>\r\n    </div>\r\n    <div class=\"card shadow mb-4\">\r\n      <div class=\"card-header py-3\">\r\n        <h6 class=\"m-0 font-weight-bold text-primary\">Orders</h6>\r\n      </div>\r\n      <div class=\"card-body\">\r\n        <div class=\"table-responsive\">\r\n          <table class=\"table table-bordered\" id=\"dataTable\" width=\"100%\" cellspacing=\"0\">\r\n            <thead>\r\n            <tr>\r\n              <th>ID</th>\r\n              <th>Invoice</th>\r\n              <th>Date</th>\r\n              <th>Number of products</th>\r\n              <th>UPADTE</th>\r\n              <th>DELETE</th>\r\n              <th>MORE</th>\r\n            </tr>\r\n            </thead>\r\n            <tfoot>\r\n            <tr>\r\n              <th>ID</th>\r\n              <th>Invoice</th>\r\n              <th>Date</th>\r\n              <th>Number of products</th>\r\n              <th>UPADTE</th>\r\n              <th>DELETE</th>\r\n              <th>MORE</th>\r\n            </tr>\r\n            </tfoot>\r\n            <tbody>\r\n            <tr v-for=\"order in orders\" :key=\"order.id\">\r\n              <th scope=\"row\">{{ order.id }}</th>\r\n              <td>{{ order.facture }}</td>\r\n              <td>{{ order.date }}</td>\r\n              <td>\r\n                0\r\n              </td>\r\n              <td> - </td>\r\n              <td>\r\n                <button class=\"btn btn-outline-danger btn-rounded\" data-mdb-ripple-color=\"dark\"\r\n                        @click=\"deleteOrder(order.id)\">Delete</button>\r\n              </td>\r\n              <th>\r\n                <button class=\"btn btn-outline-primary btn-rounded\" data-mdb-ripple-color=\"dark\"\r\n                        @click=\"redirectToProduct(order.id)\">Show Products</button>\r\n              </th>\r\n            </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n\r\n    <!-- end table-->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Oredr-page\",\r\n  data() {\r\n    return {\r\n      client: {}, // LocalStorage\r\n      orders:[],\r\n    };\r\n  },\r\n\r\n  created() {\r\n    // Get back LocalStorage\r\n    const clientData = JSON.parse(localStorage.getItem('client'));\r\n    if (clientData) {\r\n      this.client = clientData;\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    redirectToProduct(orderId) {\r\n      localStorage.setItem(\"selectedOrderId\", orderId);\r\n      this.$router.push(\"/products_order\");\r\n    },\r\n\r\n    logout() {\r\n      // Clear LocalStorage\r\n      localStorage.removeItem('client');\r\n      localStorage.setItem('loggedIn', false);\r\n      this.$router.push('/about');\r\n    },\r\n    retrieveOrder() {\r\n      if (this.client) {\r\n\r\n        axios.get(`http://localhost:8080/Client/${this.client.id}/Commande`)\r\n            .then((response) => {\r\n              this.orders = response.data;\r\n            })\r\n            .catch((error) => {\r\n              console.error(\"\", error);\r\n            });\r\n      } else {\r\n        console.error('');\r\n      }\r\n    },\r\n    deleteOrder(orderId)\r\n    {\r\n      axios.delete(`http://localhost:8080/Commande/${orderId}`)\r\n          .then((response) => {\r\n            console.log(response);\r\n            window.location.reload();\r\n          })\r\n          .catch((error) => {\r\n            console.error(\" delete failed !!! \", error);\r\n          });\r\n\r\n    }\r\n\r\n\r\n  },\r\n\r\n  mounted() {\r\n    this.retrieveOrder();\r\n\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.divider:after,\r\n.divider:before {\r\n  content: \"\";\r\n  flex: 1;\r\n  height: 1px;\r\n  background: #eee;\r\n}\r\n.name-cl\r\n{\r\n  color: midnightblue;\r\n  text-transform: uppercase;\r\n}\r\n.h-custom {\r\n  height: calc(100% - 73px);\r\n}\r\n@media (max-width: 450px) {\r\n  .h-custom {\r\n    height: 100%;\r\n  }\r\n}\r\n</style>"],"mappings":";AA0EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CAAC,CAAC;MAAE;MACZC,MAAM,EAAC;IACT,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR;IACA,MAAMC,UAAS,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC,CAAC;IAC7D,IAAIJ,UAAU,EAAE;MACd,IAAI,CAACH,MAAK,GAAIG,UAAU;IAC1B;EAEF,CAAC;EACDK,OAAO,EAAE;IACPC,iBAAiBA,CAACC,OAAO,EAAE;MACzBJ,YAAY,CAACK,OAAO,CAAC,iBAAiB,EAAED,OAAO,CAAC;MAChD,IAAI,CAACE,OAAO,CAACC,IAAI,CAAC,iBAAiB,CAAC;IACtC,CAAC;IAEDC,MAAMA,CAAA,EAAG;MACP;MACAR,YAAY,CAACS,UAAU,CAAC,QAAQ,CAAC;MACjCT,YAAY,CAACK,OAAO,CAAC,UAAU,EAAE,KAAK,CAAC;MACvC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B,CAAC;IACDG,aAAaA,CAAA,EAAG;MACd,IAAI,IAAI,CAAChB,MAAM,EAAE;QAEfH,KAAK,CAACoB,GAAG,CAAE,gCAA+B,IAAI,CAACjB,MAAM,CAACkB,EAAG,WAAU,EAC9DC,IAAI,CAAEC,QAAQ,IAAK;UAClB,IAAI,CAACnB,MAAK,GAAImB,QAAQ,CAACrB,IAAI;QAC7B,CAAC,EACAsB,KAAK,CAAEC,KAAK,IAAK;UAChBC,OAAO,CAACD,KAAK,CAAC,EAAE,EAAEA,KAAK,CAAC;QAC1B,CAAC,CAAC;MACR,OAAO;QACLC,OAAO,CAACD,KAAK,CAAC,EAAE,CAAC;MACnB;IACF,CAAC;IACDE,WAAWA,CAACd,OAAO,EACnB;MACEb,KAAK,CAAC4B,MAAM,CAAE,kCAAiCf,OAAQ,EAAC,EACnDS,IAAI,CAAEC,QAAQ,IAAK;QAClBG,OAAO,CAACG,GAAG,CAACN,QAAQ,CAAC;QACrBO,MAAM,CAACC,QAAQ,CAACC,MAAM,CAAC,CAAC;MAC1B,CAAC,EACAR,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAAC,qBAAqB,EAAEA,KAAK,CAAC;MAC7C,CAAC,CAAC;IAER;EAGF,CAAC;EAEDQ,OAAOA,CAAA,EAAG;IACR,IAAI,CAACd,aAAa,CAAC,CAAC;EAEtB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":