ources":["C:\\Users\\Firdaouss\\Desktop\\vue\\front-end\\front-end\\src\\components\\Client\\SignUpClient.vue"],"sourcesContent":["<template>\r\n  <h1>SignUp</h1>\r\n <div>\r\n   <form @submit.prevent=\"submitForm()\">\r\n     <!-- 2 column grid layout with text inputs for the first and last names -->\r\n     <div class=\"row\">\r\n       <div class=\"form-check mb-0\">\r\n         <!--input class=\"form-check-input me-2\" type=\"checkbox\" value=\"\" id=\"form2Example3\" /-->\r\n         <label class=\"form-check-label\">\r\n           <div v-if=\"errorMessage\" class=\"error-message\">\r\n             {{errorMessage}}\r\n           </div>\r\n         </label>\r\n       </div>\r\n       <div class=\"col\">\r\n         <!-- First name -->\r\n         <div class=\"form-outline mb-4\">\r\n           <label class=\"form-label\" >First name*</label>\r\n           <input type=\"text\" id=\"firstName\" class=\"form-control\" v-model=\"clientData.firstName\" required />\r\n         </div>\r\n\r\n         <!-- Last name -->\r\n         <div class=\"form-outline mb-4\">\r\n           <label class=\"form-label\" >Last name*</label>\r\n           <input type=\"text\" id=\"lastName\" class=\"form-control\" v-model=\"clientData.lastName\" required />\r\n         </div>\r\n\r\n         <div class=\"form-outline mb-4\">\r\n           <label class=\"form-label\" >Phone Number*</label>\r\n\r\n           <input type=\"number\" id=\"phoneNumber\" class=\"form-control\" v-model=\"clientData.phoneNumber\" required />\r\n         </div>\r\n         <div class=\"form-outline mb-4\">\r\n           <label class=\"form-label\" >Address*</label>\r\n\r\n           <input type=\"text\" id=\"address\" class=\"form-control\" v-model=\"clientData.address\" required />\r\n         </div>\r\n       </div>\r\n\r\n\r\n       <div class=\"col\">\r\n         <!-- Address -->\r\n\r\n\r\n         <!-- Email -->\r\n         <div class=\"form-outline mb-4\">\r\n           <label class=\"form-label\" >Email*</label>\r\n\r\n           <input type=\"email\" id=\"email\" class=\"form-control\" v-model=\"clientData.email\" required />\r\n         </div>\r\n\r\n         <div class=\"form-outline mb-4\">\r\n           <label class=\"form-label\" >Create a password*</label>\r\n\r\n           <input type=\"password\" id=\"password\" class=\"form-control\" v-model=\"clientData.password\" required />\r\n         </div>\r\n\r\n         <div class=\"form-outline mb-4\">\r\n           <label class=\"form-label\" >Repeat password*</label>\r\n\r\n           <input type=\"password\" id=\"repeatPassword\" class=\"form-control\" v-model=\"repeatPassword\" required />\r\n         </div>\r\n           <!--textarea class=\"form-control\" id=\"form6Example7\" rows=\"4\"></textarea-->\r\n       </div>\r\n     </div>\r\n\r\n\r\n\r\n     <!-- Submit button -->\r\n     <button type=\"submit\"  class=\"btn btn-primary btn-block mb-4\">Continue Registration</button>\r\n   </form>\r\n </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"SignupClient-page\",\r\n  data() {\r\n    return {\r\n      repeatPassword: \"\",\r\n      errorMessage:'',\r\n      clientData:{\r\n        firstName: \"\",\r\n        lastName: \"\",\r\n        phoneNumber: \"\",\r\n        address: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      if (\r\n          this.clientData.firstName &&\r\n          this.clientData.lastName &&\r\n          this.clientData.phoneNumber &&\r\n          this.clientData.address &&\r\n          this.clientData.email &&\r\n          this.clientData.password &&\r\n          this.repeatPassword\r\n      ) {\r\n\r\n        if(this.clientData.password == this.repeatPassword)\r\n        {\r\n          axios.post('http://localhost:8080/Client', this.clientData)\r\n              .then(response => {\r\n\r\n                console.log('Le client a été ajouté avec succès:', response.data);\r\n                const loginData = {\r\n                  email: this.clientData.email ,\r\n                  password: this.clientData.password,\r\n                };\r\n\r\n\r\n                axios.post('http://localhost:8080/Client/login', loginData)\r\n                    .then((response) => {\r\n\r\n                      console.log('login successful!', response.data);\r\n                      localStorage.setItem('client',JSON.stringify(response.data));\r\n                      localStorage.setItem('loggedIn', true);\r\n                      this.$router.push('/');\r\n                    })\r\n\r\n              })\r\n              .catch(error => {\r\n                console.error('Une erreur s\\'est produite lors de l\\'ajout du client:', error);\r\n              });\r\n        }\r\n\r\n      } else {\r\n\r\n        alert(\"please enter correctly!!\");\r\n        this.errorMessage='please enter correctly !!';\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.divider:after,\r\n.divider:before {\r\n  content: \"\";\r\n  flex: 1;\r\n  height: 1px;\r\n  background: #eee;\r\n}\r\n</style>\r\n"],"mappings":";AA4EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAC,EAAE;MACfC,UAAU,EAAC;QACTC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,WAAW,EAAE,EAAE;QACfC,OAAO,EAAE,EAAE;QACXC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IACI,IAAI,CAACR,UAAU,CAACC,SAAQ,IACxB,IAAI,CAACD,UAAU,CAACE,QAAO,IACvB,IAAI,CAACF,UAAU,CAACG,WAAU,IAC1B,IAAI,CAACH,UAAU,CAACI,OAAM,IACtB,IAAI,CAACJ,UAAU,CAACK,KAAI,IACpB,IAAI,CAACL,UAAU,CAACM,QAAO,IACvB,IAAI,CAACR,cAAa,EACpB;QAEA,IAAG,IAAI,CAACE,UAAU,CAACM,QAAO,IAAK,IAAI,CAACR,cAAc,EAClD;UACEH,KAAK,CAACc,IAAI,CAAC,8BAA8B,EAAE,IAAI,CAACT,UAAU,EACrDU,IAAI,CAACC,QAAO,IAAK;YAEhBC,OAAO,CAACC,GAAG,CAAC,qCAAqC,EAAEF,QAAQ,CAACd,IAAI,CAAC;YACjE,MAAMiB,SAAQ,GAAI;cAChBT,KAAK,EAAE,IAAI,CAACL,UAAU,C